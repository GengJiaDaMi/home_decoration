/**app.wxss**/
.container{
  width: 100%;
}
.navigator-hover{
  color:#818181;
  background-color:rgba(0, 0, 0, 0);
  opacity: 1;
}
.phcolor{
  color: #999;
}
.no-more {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #b2b2b2;
}
.no-more::after,
.no-more::before {
  content: '';
  display: block;
  width: 100rpx;
  height: 2rpx;
  background: #e1e1e1;
}